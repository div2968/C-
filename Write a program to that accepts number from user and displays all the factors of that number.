#include <iostream>

int main() {
    int number;
    std::cout << "Enter a positive integer: ";
    std::cin >> number;

    if (number <= 0) {
        std::cout << "Invalid input. Please enter a positive integer." << std::endl;
        return 1; // Exit program with error status
    }

    std::cout << "Factors of " << number << " are:" << std::endl;
    for (int i = 1; i <= number; ++i) {
        if (number % i == 0) {
            std::cout << i << " ";
        }
    }
    std::cout << std::endl;

    return 0;
}
